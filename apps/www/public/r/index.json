  {
    "name": "typing-indicator",
    "type": "registry:ui",
    "files": [
      {
        "path": "ui/typing-indicator.tsx",
        "type": "registry:ui"
      }
    ],
    "tailwind": {
      "config": {
        "theme": {
          "extend": {
            "keyframes": {
              "typing-dot-bounce": {
